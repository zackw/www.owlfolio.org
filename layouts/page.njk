{% extends "base.njk" %}
{% block title %}{{ title }} | {{ sitename }}{% endblock %}
{% block content %}
<article id="content" class="body">
  <header class="page-header">
    <h1 class="entry-title">{{ title }}</h1>
  </header>
  <div class="entry-content">
    {{ contents | safe }}
  </div>
</article>
{% endblock %}
